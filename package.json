{
  "name": "typescript-boilerplate",
  "version": "1.0.0",
  "description": "typescript脚手架",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --mode development --config webpack.dev.js",
    "build:test": "NODE_ENV=test webpack --mode production --config webpack.prod.js",
    "build:development": "NODE_ENV=development webpack --mode production --config webpack.prod.js",
    "build:intranet": "NODE_ENV=intranet webpack --mode production --config webpack.prod.js",
    "build:production": "NODE_ENV=production webpack --mode production  --config webpack.prod.js",
    "standardfix": "standardx --fix",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "keywords": [],
  "author": "luhk",
  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "@loadable/component": "^5.10.3",
    "antd": "^4.4.2",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "css-vars": "^2.2.0",
    "events": "^3.1.0",
    "history": "^4.10.1",
    "moment": "^2.24.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-loadable": "5.5.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "@types/events": "^3.0.0",
    "@types/history": "^4.7.6",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-import": "1.13.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "3.0.0",
    "commitlint": "9.0.1",
    "core-js": "^3.6.5",
    "cross-env": "7.0.2",
    "cross-spawn": "7.0.3",
    "css-loader": "3.6.0",
    "detect-port": "1.3.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-react": "^7.20.3",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "less": "3.11.3",
    "less-loader": "6.1.3",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass-loader": "8.0.2",
    "source-map-loader": "^1.0.0",
    "style-loader": "1.2.1",
